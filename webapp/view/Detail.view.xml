		</sections>
	</ObjectPageLayout>
		<semantic:customShareMenuContent>
			<m:Button 
			text="{i18n>contact}" 
			icon="sap-icon://email"
			press="onSuggestionPress" />
		</semantic:customShareMenuContent>
	<semantic:content>
	    <m:Image width="140px" height="140px" id="__image1"/>
	    <m:Image width="140px" height="140px" id="__image2"/>
	    <m:Image width="140px" height="140px" id="__image3"/>
	    <m:Image width="140px" height="140px" id="__image5"/>
	</semantic:content>
	</semantic:DetailPage>
</mvc:View>